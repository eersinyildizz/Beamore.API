                        </a>
                        <ul class="treeview-menu">
                            <li><i class="fa fa-edit"></i><a href="#">New Event</a></li>
                            <li><i class="fa fa-edit"></i><a href="#">My Events</a></li>
                        </ul>
                    </li>*@
                </ul>
                <!-- /.sidebar-menu -->
            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                @RenderBody()
            </section>
            <!-- Main content -->
            <section class="content">
                <!-- Your Page Content Here -->
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <!-- Main Footer -->
        <footer class="main-footer">
            <!-- To the right -->
            <div class="pull-right hidden-xs">
                Anything you want
            </div>
            <!-- Default to the left -->
            <strong>Copyright &copy; 2017 <a href="http://beamoreweb.azurewebsites.net/" target="_blank">Beamore</a>.</strong> All rights reserved.
        </footer>
        <!-- Control Sidebar -->
    
        <!-- /.control-sidebar -->
        <!-- Add the sidebar's background. This div must be placed
             immediately after the control sidebar -->
        <div class="control-sidebar-bg"></div>
    </div>
